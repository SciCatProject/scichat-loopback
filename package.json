{
  "name": "scichat-loopback",
  "version": "0.0.1",
  "description": "Wrapper for Synapse API",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha -r dotenv/config --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t scichat-loopback .",
    "docker:run": "docker run -p 3000:3000 -d scichat-loopback",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r dotenv/config -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Henrik Johansson <henrik.johansson@ess.eu>",
  "license": "GPL-3.0",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.4",
    "@loopback/boot": "^3.4.4",
    "@loopback/core": "^2.18.0",
    "@loopback/repository": "^3.7.3",
    "@loopback/rest": "^9.3.1",
    "@loopback/rest-explorer": "^4.1.2",
    "@loopback/service-proxy": "^3.2.4",
    "@user-office-software/duo-message-broker": "^1.3.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.6.0",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mongodb": "^6.2.0",
    "loopback-connector-rest": "^4.0.1",
    "snyk": "^1.914.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.0",
    "@loopback/eslint-config": "^10.1.0",
    "@loopback/testlab": "^3.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/debug": "^4.1.7",
    "@types/node": "^10.17.55",
    "eslint": "^7.22.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.2.3"
  },
  "snyk": true
}
