{
  "name": "scichat-loopback",
  "version": "1.0.0",
  "description": "Loopback implementation of the SciChat client.",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t scichat-loopback .",
    "docker:run": "docker run -p 3000:3000 -d scichat-loopback",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.0.15",
    "@loopback/boot": "^1.1.0",
    "@loopback/context": "^1.7.0",
    "@loopback/core": "^1.1.8",
    "@loopback/openapi-v3": "^1.3.0",
    "@loopback/repository": "^1.1.8",
    "@loopback/rest": "^1.8.0",
    "@loopback/rest-explorer": "^1.1.11",
    "@loopback/service-proxy": "^1.1.1",
    "@types/chai": "^4.1.7",
    "loopback-component-storage": "^3.6.0",
    "loopback-connector-mongodb": "^4.0.0",
    "loopback-connector-rest": "^3.4.1",
    "mongodb": "^3.1.13",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "request-promise": "^4.2.4",
    "superagent": "^4.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.2",
    "@loopback/testlab": "^1.1.0",
    "@loopback/tslint-config": "^2.0.2",
    "@types/node": "^10.11.2",
    "@types/passport": "^1.0.0",
    "@types/passport-http": "^0.3.7",
    "bluebird": "^3.5.3",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "sinon": "^7.3.0",
    "tslint": "^5.12.0",
    "typescript": "^3.3.1"
  }
}
